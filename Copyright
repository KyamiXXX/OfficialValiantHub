local KeyGuardLibrary = loadstring(game:HttpGet("https://cdn.keyguardian.org/library/v1.0.0.lua"))()
local trueData = "47d1f59bfedc48c29c788568e6ea8984"
local falseData = "1ba06edf85934127803827a187a76859"

KeyGuardLibrary.Set({
  publicToken = "f1ec5979600349cc8827610abfa86696",
  privateToken = "3186ddb6ee4a49a285eadd8dccb91c49",
  trueData = trueData,
  falseData = falseData,
})

local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local key = ""

local Window = Fluent:CreateWindow({
    Title = "Key System",
    SubTitle = "ValiantHub",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 340),
    Acrylic = false,
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl
})

local Tabs = {
    KeySys = Window:AddTab({ Title = "Key System", Icon = "key" }),
}

local Entkey = Tabs.KeySys:AddInput("Input", {
    Title = "Enter Key",
    Description = "Enter Key Here",
    Default = "",
    Placeholder = "Enter keyâ€¦",
    Numeric = false,
    Finished = false,
    Callback = function(Value)
        key = Value
    end
})

local Checkkey = Tabs.KeySys:AddButton({
    Title = "Check Key",
    Description = "Enter Key before pressing this button",
    Callback = function()
        local response = KeyGuardLibrary.validateDefaultKey(key)
        if response == trueData then
           print("Key is valid")
           -- Your code here
        else
           print("Key is invalid")
        end
    end
})

local Getkey = Tabs.KeySys:AddButton({
    Title = "Get Key",
    Description = "Get Key here",
    Callback = function()
       setclipboard(KeyGuardLibrary.getLink())
    end
})

Window:SelectTab(1)

local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
    Name = "VALIANT SCRIPT - BETA",
    LoadingTitle = "TOP 1 DH SCRIPT",
    LoadingSubtitle = "by #opprapier in dc",
    KeySystem = false,
    KeySettings = {
        Title = "Key | Valiant Hub",
        Subtitle = "Key System",
        Note = "Key In Discord Server",
        FileName = "valientXzKeysystem",
        SaveKey = false,
        GrabKeyFromSite = true,
        Key = {"https://pastebin.com/raw/JXh4Pj26", "https://pastebin.com/raw/zNhEvEe7"}
    }
})

local MainTab = Window:CreateTab("Home", nil)
MainTab:CreateSection("Scripts!")

Rayfield:Notify({
    Title = "You executed the script",
    Content = "Very cool GUI",
    Duration = 5,
    Image = 13047715178,
    Actions = {
        Ignore = {
            Name = "Okay!",
            Callback = function()
                print("The user tapped Okay!")
            end
        }
    }
})

